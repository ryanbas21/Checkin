{
    "compilerOptions": {
        "module": "es2015",
        "target": "ESNEXT",
        "noImplicitAny": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "jsx": "react",
        "compileOnSave": true,
        "compilerOptions": {
          "noImplicitAny" : true
        },
        "experimentalDecorators": true,
        "allowJs": true
    }
}
